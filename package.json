{
  "name": "react-redux-scaffold",
  "version": "2.0.0",
  "description": "React Redux Scaffold",
  "main": "src/index.tsx",
  "scripts": {
    "format": "prettier --write **/*.jsx **/*.js **/*.tsx **/*.ts **/*.css **/*.json",
    "lint": "run-p lint:*",
    "lint:js": "eslint **/*.jsx **/*.js **/*.tsx **/*.ts",
    "lint:css": "stylelint ./src/**/*.jsx ./src/**/*.js ./src/**/*.tsx ./src/**/*.ts",
    "fl": "run-s format lint",
    "gendb": "node genDb.js > db.json",
    "mock": "json-server --watch db.json",
    "serve": "run-s gendb mock",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --hot --config webpack.dev.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaitoy/react-redux-scaffold.git"
  },
  "author": "kaitoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaitoy/react-redux-scaffold/issues"
  },
  "homepage": "https://github.com/kaitoy/react-redux-scaffold",
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@material-ui/core": "^4.9.2",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "immer": "^5.3.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.1",
    "typeface-roboto": "^0.0.75"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.7.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.4.3",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0 || ^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "faker": "^4.1.0",
    "file-loader": "^5.0.2",
    "hard-source-webpack-plugin": "^0.13.1",
